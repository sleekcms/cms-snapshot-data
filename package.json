{
  "name": "cms-snapshot-data",
  "version": "1.0.0",
  "description": "SleekCMS Snapshot Data",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleekcms/cms-snapshot-data.git"
  },
  "scripts": {
    "clean": "rimraf dist esm",
    "prebuild": "npm run clean",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags",
    "test": "jest",
    "build:esm": "tsc --target es2022 --outDir esm",
    "build:cjs": "tsc --target es2018 --module commonjs --outDir dist",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "author": "Yusuf Bhabhrawala",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/jsonfile": "^6.1.1",
    "jest": "^29.2.1",
    "jsonfile": "^6.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.1",
    "@sleeksky/alt-schema": "^2.1.0",
    "@types/marked": "^4.3.0",
    "lodash": "^4.17.21",
    "marked": "^5.0.1",
    "randexp": "^0.5.3"
  }
}
